{
  "name": "MyAsap Android Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "redhat.java",
    "vscjava.vscode-java-pack",
    "ms-vscode.cpptools",
    "github.copilot-chat"
  ],
  "postCreateCommand": "bash /workspace/.devcontainer/setup-android-sdk.sh",
  "remoteUser": "vscode",
  "workspaceFolder": "/workspace"
}