// .devcontainer/devcontainer.json
{
  "name": "MyAsap Android Dev",

  "build": {
    "dockerfile": "Dockerfile"
  },

  "customizations": {
    "vscode": {
      "extensions": [
        "redhat.java",
        "vscjava.vscode-java-pack",
        "ms-vscode.cpptools",
        "github.copilot-chat"
      ],
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      }
    }
  },

  "remoteUser": "vscode",
  "workspaceFolder": "/workspaces/MyAsapNewversion"
}